<span id="L99" class="ln">    99&nbsp;&nbsp;</span>func (e alert) String() string {
<span id="L100" class="ln">   100&nbsp;&nbsp;</span>	s, ok := alertText[e]
<span id="L101" class="ln">   101&nbsp;&nbsp;</span>	if ok {
<span id="L102" class="ln">   102&nbsp;&nbsp;</span>		return &#34;tls: &#34; + s
<span id="L103" class="ln">   103&nbsp;&nbsp;</span>	}
<span id="L104" class="ln">   104&nbsp;&nbsp;</span>	return &#34;tls: alert(&#34; + strconv.Itoa(int(e)) + &#34;)&#34;
<span id="L105" class="ln">   105&nbsp;&nbsp;</span>}
<span id="L106" class="ln">   106&nbsp;&nbsp;</span>
