<span id="L3" class="ln">     3&nbsp;&nbsp;</span><span class="comment">// Copyright 2013 The Go Authors. All rights reserved.</span>
<span id="L4" class="ln">     4&nbsp;&nbsp;</span><span class="comment">// Use of this source code is governed by a BSD-style</span>
<span id="L5" class="ln">     5&nbsp;&nbsp;</span><span class="comment">// license that can be found in the LICENSE file.</span>
<span id="L6" class="ln">     6&nbsp;&nbsp;</span>
<span id="L7" class="ln">     7&nbsp;&nbsp;</span><span class="comment">// This file implements Sizes.</span>
<span id="L8" class="ln">     8&nbsp;&nbsp;</span>
<span id="L9" class="ln">     9&nbsp;&nbsp;</span>package types
<span id="L10" class="ln">    10&nbsp;&nbsp;</span>
<span id="L11" class="ln">    11&nbsp;&nbsp;</span><span class="comment">// Sizes defines the sizing functions for package unsafe.</span>
<span id="L12" class="ln">    12&nbsp;&nbsp;</span>type Sizes interface {
<span id="L13" class="ln">    13&nbsp;&nbsp;</span>	<span class="comment">// Alignof returns the alignment of a variable of type T.</span>
<span id="L14" class="ln">    14&nbsp;&nbsp;</span>	<span class="comment">// Alignof must implement the alignment guarantees required by the spec.</span>
<span id="L15" class="ln">    15&nbsp;&nbsp;</span>	<span class="comment">// The result must be &gt;= 1.</span>
<span id="L16" class="ln">    16&nbsp;&nbsp;</span>	Alignof(T Type) int64
<span id="L17" class="ln">    17&nbsp;&nbsp;</span>
<span id="L18" class="ln">    18&nbsp;&nbsp;</span>	<span class="comment">// Offsetsof returns the offsets of the given struct fields, in bytes.</span>
<span id="L19" class="ln">    19&nbsp;&nbsp;</span>	<span class="comment">// Offsetsof must implement the offset guarantees required by the spec.</span>
<span id="L20" class="ln">    20&nbsp;&nbsp;</span>	<span class="comment">// A negative entry in the result indicates that the struct is too large.</span>
<span id="L21" class="ln">    21&nbsp;&nbsp;</span>	Offsetsof(fields []*Var) []int64
<span id="L22" class="ln">    22&nbsp;&nbsp;</span>
<span id="L23" class="ln">    23&nbsp;&nbsp;</span>	<span class="comment">// Sizeof returns the size of a variable of type T.</span>
<span id="L24" class="ln">    24&nbsp;&nbsp;</span>	<span class="comment">// Sizeof must implement the size guarantees required by the spec.</span>
<span id="L25" class="ln">    25&nbsp;&nbsp;</span>	<span class="comment">// A negative result indicates that T is too large.</span>
<span id="L26" class="ln">    26&nbsp;&nbsp;</span>	Sizeof(T Type) int64
<span id="L27" class="ln">    27&nbsp;&nbsp;</span>}
<span id="L28" class="ln">    28&nbsp;&nbsp;</span>
<span id="L29" class="ln">    29&nbsp;&nbsp;</span><span class="comment">// StdSizes is a convenience type for creating commonly used Sizes.</span>
<span id="L30" class="ln">    30&nbsp;&nbsp;</span><span class="comment">// It makes the following simplifying assumptions:</span>
<span id="L31" class="ln">    31&nbsp;&nbsp;</span><span class="comment">//</span>
<span id="L32" class="ln">    32&nbsp;&nbsp;</span><span class="comment">//   - The size of explicitly sized basic types (int16, etc.) is the</span>
<span id="L33" class="ln">    33&nbsp;&nbsp;</span><span class="comment">//     specified size.</span>
<span id="L34" class="ln">    34&nbsp;&nbsp;</span><span class="comment">//   - The size of strings and interfaces is 2*WordSize.</span>
<span id="L35" class="ln">    35&nbsp;&nbsp;</span><span class="comment">//   - The size of slices is 3*WordSize.</span>
<span id="L36" class="ln">    36&nbsp;&nbsp;</span><span class="comment">//   - The size of an array of n elements corresponds to the size of</span>
<span id="L37" class="ln">    37&nbsp;&nbsp;</span><span class="comment">//     a struct of n consecutive fields of the array&#39;s element type.</span>
<span id="L38" class="ln">    38&nbsp;&nbsp;</span><span class="comment">//   - The size of a struct is the offset of the last field plus that</span>
<span id="L39" class="ln">    39&nbsp;&nbsp;</span><span class="comment">//     field&#39;s size. As with all element types, if the struct is used</span>
<span id="L40" class="ln">    40&nbsp;&nbsp;</span><span class="comment">//     in an array its size must first be aligned to a multiple of the</span>
<span id="L41" class="ln">    41&nbsp;&nbsp;</span><span class="comment">//     struct&#39;s alignment.</span>
<span id="L42" class="ln">    42&nbsp;&nbsp;</span><span class="comment">//   - All other types have size WordSize.</span>
<span id="L43" class="ln">    43&nbsp;&nbsp;</span><span class="comment">//   - Arrays and structs are aligned per spec definition; all other</span>
<span id="L44" class="ln">    44&nbsp;&nbsp;</span><span class="comment">//     types are naturally aligned with a maximum alignment MaxAlign.</span>
<span id="L45" class="ln">    45&nbsp;&nbsp;</span><span class="comment">//</span>
<span id="L46" class="ln">    46&nbsp;&nbsp;</span><span class="comment">// *StdSizes implements Sizes.</span>
<span id="L47" class="ln">    47&nbsp;&nbsp;</span>type StdSizes struct {
<span id="L48" class="ln">    48&nbsp;&nbsp;</span>	WordSize int64 <span class="comment">// word size in bytes - must be &gt;= 4 (32bits)</span>
<span id="L49" class="ln">    49&nbsp;&nbsp;</span>	MaxAlign int64 <span class="comment">// maximum alignment in bytes - must be &gt;= 1</span>
<span id="L50" class="ln">    50&nbsp;&nbsp;</span>}
<span id="L51" class="ln">    51&nbsp;&nbsp;</span>
<span id="L52" class="ln">    52&nbsp;&nbsp;</span><span class="selection">func (s *StdSizes) Alignof(T Type) (result int64)</span> {
<span id="L53" class="ln">    53&nbsp;&nbsp;</span>	defer func() {
<span id="L54" class="ln">    54&nbsp;&nbsp;</span>		assert(result &gt;= 1)
<span id="L55" class="ln">    55&nbsp;&nbsp;</span>	}()
<span id="L56" class="ln">    56&nbsp;&nbsp;</span>
<span id="L57" class="ln">    57&nbsp;&nbsp;</span>	<span class="comment">// For arrays and structs, alignment is defined in terms</span>
<span id="L58" class="ln">    58&nbsp;&nbsp;</span>	<span class="comment">// of alignment of the elements and fields, respectively.</span>
<span id="L59" class="ln">    59&nbsp;&nbsp;</span>	switch t := under(T).(type) {
<span id="L60" class="ln">    60&nbsp;&nbsp;</span>	case *Array:
<span id="L61" class="ln">    61&nbsp;&nbsp;</span>		<span class="comment">// spec: &#34;For a variable x of array type: unsafe.Alignof(x)</span>
<span id="L62" class="ln">    62&nbsp;&nbsp;</span>		<span class="comment">// is the same as unsafe.Alignof(x[0]), but at least 1.&#34;</span>
<span id="L63" class="ln">    63&nbsp;&nbsp;</span>		return s.Alignof(t.elem)
<span id="L64" class="ln">    64&nbsp;&nbsp;</span>	case *Struct:
<span id="L65" class="ln">    65&nbsp;&nbsp;</span>		if len(t.fields) == 0 &amp;&amp; _IsSyncAtomicAlign64(T) {
<span id="L66" class="ln">    66&nbsp;&nbsp;</span>			<span class="comment">// Special case: sync/atomic.align64 is an</span>
<span id="L67" class="ln">    67&nbsp;&nbsp;</span>			<span class="comment">// empty struct we recognize as a signal that</span>
<span id="L68" class="ln">    68&nbsp;&nbsp;</span>			<span class="comment">// the struct it contains must be</span>
<span id="L69" class="ln">    69&nbsp;&nbsp;</span>			<span class="comment">// 64-bit-aligned.</span>
<span id="L70" class="ln">    70&nbsp;&nbsp;</span>			<span class="comment">//</span>
<span id="L71" class="ln">    71&nbsp;&nbsp;</span>			<span class="comment">// This logic is equivalent to the logic in</span>
<span id="L72" class="ln">    72&nbsp;&nbsp;</span>			<span class="comment">// cmd/compile/internal/types/size.go:calcStructOffset</span>
<span id="L73" class="ln">    73&nbsp;&nbsp;</span>			return 8
<span id="L74" class="ln">    74&nbsp;&nbsp;</span>		}
<span id="L75" class="ln">    75&nbsp;&nbsp;</span>
<span id="L76" class="ln">    76&nbsp;&nbsp;</span>		<span class="comment">// spec: &#34;For a variable x of struct type: unsafe.Alignof(x)</span>
<span id="L77" class="ln">    77&nbsp;&nbsp;</span>		<span class="comment">// is the largest of the values unsafe.Alignof(x.f) for each</span>
<span id="L78" class="ln">    78&nbsp;&nbsp;</span>		<span class="comment">// field f of x, but at least 1.&#34;</span>
<span id="L79" class="ln">    79&nbsp;&nbsp;</span>		max := int64(1)
<span id="L80" class="ln">    80&nbsp;&nbsp;</span>		for _, f := range t.fields {
<span id="L81" class="ln">    81&nbsp;&nbsp;</span>			if a := s.Alignof(f.typ); a &gt; max {
<span id="L82" class="ln">    82&nbsp;&nbsp;</span>				max = a
<span id="L83" class="ln">    83&nbsp;&nbsp;</span>			}
<span id="L84" class="ln">    84&nbsp;&nbsp;</span>		}
<span id="L85" class="ln">    85&nbsp;&nbsp;</span>		return max
<span id="L86" class="ln">    86&nbsp;&nbsp;</span>	case *Slice, *Interface:
<span id="L87" class="ln">    87&nbsp;&nbsp;</span>		<span class="comment">// Multiword data structures are effectively structs</span>
<span id="L88" class="ln">    88&nbsp;&nbsp;</span>		<span class="comment">// in which each element has size WordSize.</span>
<span id="L89" class="ln">    89&nbsp;&nbsp;</span>		<span class="comment">// Type parameters lead to variable sizes/alignments;</span>
<span id="L90" class="ln">    90&nbsp;&nbsp;</span>		<span class="comment">// StdSizes.Alignof won&#39;t be called for them.</span>
<span id="L91" class="ln">    91&nbsp;&nbsp;</span>		assert(!isTypeParam(T))
<span id="L92" class="ln">    92&nbsp;&nbsp;</span>		return s.WordSize
<span id="L93" class="ln">    93&nbsp;&nbsp;</span>	case *Basic:
<span id="L94" class="ln">    94&nbsp;&nbsp;</span>		<span class="comment">// Strings are like slices and interfaces.</span>
<span id="L95" class="ln">    95&nbsp;&nbsp;</span>		if t.Info()&amp;IsString != 0 {
<span id="L96" class="ln">    96&nbsp;&nbsp;</span>			return s.WordSize
<span id="L97" class="ln">    97&nbsp;&nbsp;</span>		}
<span id="L98" class="ln">    98&nbsp;&nbsp;</span>	case *TypeParam, *Union:
<span id="L99" class="ln">    99&nbsp;&nbsp;</span>		unreachable()
<span id="L100" class="ln">   100&nbsp;&nbsp;</span>	}
<span id="L101" class="ln">   101&nbsp;&nbsp;</span>	a := s.Sizeof(T) <span class="comment">// may be 0 or negative</span>
<span id="L102" class="ln">   102&nbsp;&nbsp;</span>	<span class="comment">// spec: &#34;For a variable x of any type: unsafe.Alignof(x) is at least 1.&#34;</span>
<span id="L103" class="ln">   103&nbsp;&nbsp;</span>	if a &lt; 1 {
<span id="L104" class="ln">   104&nbsp;&nbsp;</span>		return 1
<span id="L105" class="ln">   105&nbsp;&nbsp;</span>	}
<span id="L106" class="ln">   106&nbsp;&nbsp;</span>	<span class="comment">// complex{64,128} are aligned like [2]float{32,64}.</span>
<span id="L107" class="ln">   107&nbsp;&nbsp;</span>	if isComplex(T) {
<span id="L108" class="ln">   108&nbsp;&nbsp;</span>		a /= 2
<span id="L109" class="ln">   109&nbsp;&nbsp;</span>	}
<span id="L110" class="ln">   110&nbsp;&nbsp;</span>	if a &gt; s.MaxAlign {
<span id="L111" class="ln">   111&nbsp;&nbsp;</span>		return s.MaxAlign
<span id="L112" class="ln">   112&nbsp;&nbsp;</span>	}
<span id="L113" class="ln">   113&nbsp;&nbsp;</span>	return a
<span id="L114" class="ln">   114&nbsp;&nbsp;</span>}
<span id="L115" class="ln">   115&nbsp;&nbsp;</span>
<span id="L116" class="ln">   116&nbsp;&nbsp;</span>func _IsSyncAtomicAlign64(T Type) bool {
<span id="L117" class="ln">   117&nbsp;&nbsp;</span>	named := asNamed(T)
<span id="L118" class="ln">   118&nbsp;&nbsp;</span>	if named == nil {
<span id="L119" class="ln">   119&nbsp;&nbsp;</span>		return false
<span id="L120" class="ln">   120&nbsp;&nbsp;</span>	}
<span id="L121" class="ln">   121&nbsp;&nbsp;</span>	obj := named.Obj()
<span id="L122" class="ln">   122&nbsp;&nbsp;</span>	return obj.Name() == &#34;align64&#34; &amp;&amp;
<span id="L123" class="ln">   123&nbsp;&nbsp;</span>		obj.Pkg() != nil &amp;&amp;
<span id="L124" class="ln">   124&nbsp;&nbsp;</span>		(obj.Pkg().Path() == &#34;sync/atomic&#34; ||
<span id="L125" class="ln">   125&nbsp;&nbsp;</span>			obj.Pkg().Path() == &#34;runtime/internal/atomic&#34;)
<span id="L126" class="ln">   126&nbsp;&nbsp;</span>}
<span id="L127" class="ln">   127&nbsp;&nbsp;</span>
<span id="L128" class="ln">   128&nbsp;&nbsp;</span>func (s *StdSizes) Offsetsof(fields []*Var) []int64 {
<span id="L129" class="ln">   129&nbsp;&nbsp;</span>	offsets := make([]int64, len(fields))
<span id="L130" class="ln">   130&nbsp;&nbsp;</span>	var offs int64
<span id="L131" class="ln">   131&nbsp;&nbsp;</span>	for i, f := range fields {
<span id="L132" class="ln">   132&nbsp;&nbsp;</span>		if offs &lt; 0 {
<span id="L133" class="ln">   133&nbsp;&nbsp;</span>			<span class="comment">// all remaining offsets are too large</span>
<span id="L134" class="ln">   134&nbsp;&nbsp;</span>			offsets[i] = -1
<span id="L135" class="ln">   135&nbsp;&nbsp;</span>			continue
<span id="L136" class="ln">   136&nbsp;&nbsp;</span>		}
<span id="L137" class="ln">   137&nbsp;&nbsp;</span>		<span class="comment">// offs &gt;= 0</span>
<span id="L138" class="ln">   138&nbsp;&nbsp;</span>		a := s.Alignof(f.typ)
<span id="L139" class="ln">   139&nbsp;&nbsp;</span>		offs = align(offs, a) <span class="comment">// possibly &lt; 0 if align overflows</span>
<span id="L140" class="ln">   140&nbsp;&nbsp;</span>		offsets[i] = offs
<span id="L141" class="ln">   141&nbsp;&nbsp;</span>		if d := s.Sizeof(f.typ); d &gt;= 0 &amp;&amp; offs &gt;= 0 {
<span id="L142" class="ln">   142&nbsp;&nbsp;</span>			offs += d <span class="comment">// ok to overflow to &lt; 0</span>
<span id="L143" class="ln">   143&nbsp;&nbsp;</span>		} else {
<span id="L144" class="ln">   144&nbsp;&nbsp;</span>			offs = -1 <span class="comment">// f.typ or offs is too large</span>
<span id="L145" class="ln">   145&nbsp;&nbsp;</span>		}
<span id="L146" class="ln">   146&nbsp;&nbsp;</span>	}
<span id="L147" class="ln">   147&nbsp;&nbsp;</span>	return offsets
<span id="L148" class="ln">   148&nbsp;&nbsp;</span>}
<span id="L149" class="ln">   149&nbsp;&nbsp;</span>
<span id="L150" class="ln">   150&nbsp;&nbsp;</span>var basicSizes = [...]byte{
<span id="L151" class="ln">   151&nbsp;&nbsp;</span>	Bool:       1,
<span id="L152" class="ln">   152&nbsp;&nbsp;</span>	Int8:       1,
<span id="L153" class="ln">   153&nbsp;&nbsp;</span>	Int16:      2,
<span id="L154" class="ln">   154&nbsp;&nbsp;</span>	Int32:      4,
<span id="L155" class="ln">   155&nbsp;&nbsp;</span>	Int64:      8,
<span id="L156" class="ln">   156&nbsp;&nbsp;</span>	Uint8:      1,
<span id="L157" class="ln">   157&nbsp;&nbsp;</span>	Uint16:     2,
<span id="L158" class="ln">   158&nbsp;&nbsp;</span>	Uint32:     4,
<span id="L159" class="ln">   159&nbsp;&nbsp;</span>	Uint64:     8,
<span id="L160" class="ln">   160&nbsp;&nbsp;</span>	Float32:    4,
<span id="L161" class="ln">   161&nbsp;&nbsp;</span>	Float64:    8,
<span id="L162" class="ln">   162&nbsp;&nbsp;</span>	Complex64:  8,
<span id="L163" class="ln">   163&nbsp;&nbsp;</span>	Complex128: 16,
<span id="L164" class="ln">   164&nbsp;&nbsp;</span>}
<span id="L165" class="ln">   165&nbsp;&nbsp;</span>
<span id="L166" class="ln">   166&nbsp;&nbsp;</span>func (s *StdSizes) Sizeof(T Type) int64 {
<span id="L167" class="ln">   167&nbsp;&nbsp;</span>	switch t := under(T).(type) {
<span id="L168" class="ln">   168&nbsp;&nbsp;</span>	case *Basic:
<span id="L169" class="ln">   169&nbsp;&nbsp;</span>		assert(isTyped(T))
<span id="L170" class="ln">   170&nbsp;&nbsp;</span>		k := t.kind
<span id="L171" class="ln">   171&nbsp;&nbsp;</span>		if int(k) &lt; len(basicSizes) {
<span id="L172" class="ln">   172&nbsp;&nbsp;</span>			if s := basicSizes[k]; s &gt; 0 {
<span id="L173" class="ln">   173&nbsp;&nbsp;</span>				return int64(s)
<span id="L174" class="ln">   174&nbsp;&nbsp;</span>			}
<span id="L175" class="ln">   175&nbsp;&nbsp;</span>		}
<span id="L176" class="ln">   176&nbsp;&nbsp;</span>		if k == String {
<span id="L177" class="ln">   177&nbsp;&nbsp;</span>			return s.WordSize * 2
<span id="L178" class="ln">   178&nbsp;&nbsp;</span>		}
<span id="L179" class="ln">   179&nbsp;&nbsp;</span>	case *Array:
<span id="L180" class="ln">   180&nbsp;&nbsp;</span>		n := t.len
<span id="L181" class="ln">   181&nbsp;&nbsp;</span>		if n &lt;= 0 {
<span id="L182" class="ln">   182&nbsp;&nbsp;</span>			return 0
<span id="L183" class="ln">   183&nbsp;&nbsp;</span>		}
<span id="L184" class="ln">   184&nbsp;&nbsp;</span>		<span class="comment">// n &gt; 0</span>
<span id="L185" class="ln">   185&nbsp;&nbsp;</span>		esize := s.Sizeof(t.elem)
<span id="L186" class="ln">   186&nbsp;&nbsp;</span>		if esize &lt; 0 {
<span id="L187" class="ln">   187&nbsp;&nbsp;</span>			return -1 <span class="comment">// element too large</span>
<span id="L188" class="ln">   188&nbsp;&nbsp;</span>		}
<span id="L189" class="ln">   189&nbsp;&nbsp;</span>		if esize == 0 {
<span id="L190" class="ln">   190&nbsp;&nbsp;</span>			return 0 <span class="comment">// 0-size element</span>
<span id="L191" class="ln">   191&nbsp;&nbsp;</span>		}
<span id="L192" class="ln">   192&nbsp;&nbsp;</span>		<span class="comment">// esize &gt; 0</span>
<span id="L193" class="ln">   193&nbsp;&nbsp;</span>		a := s.Alignof(t.elem)
<span id="L194" class="ln">   194&nbsp;&nbsp;</span>		ea := align(esize, a) <span class="comment">// possibly &lt; 0 if align overflows</span>
<span id="L195" class="ln">   195&nbsp;&nbsp;</span>		if ea &lt; 0 {
<span id="L196" class="ln">   196&nbsp;&nbsp;</span>			return -1
<span id="L197" class="ln">   197&nbsp;&nbsp;</span>		}
<span id="L198" class="ln">   198&nbsp;&nbsp;</span>		<span class="comment">// ea &gt;= 1</span>
<span id="L199" class="ln">   199&nbsp;&nbsp;</span>		n1 := n - 1 <span class="comment">// n1 &gt;= 0</span>
<span id="L200" class="ln">   200&nbsp;&nbsp;</span>		<span class="comment">// Final size is ea*n1 + esize; and size must be &lt;= maxInt64.</span>
<span id="L201" class="ln">   201&nbsp;&nbsp;</span>		const maxInt64 = 1&lt;&lt;63 - 1
<span id="L202" class="ln">   202&nbsp;&nbsp;</span>		if n1 &gt; 0 &amp;&amp; ea &gt; maxInt64/n1 {
<span id="L203" class="ln">   203&nbsp;&nbsp;</span>			return -1 <span class="comment">// ea*n1 overflows</span>
<span id="L204" class="ln">   204&nbsp;&nbsp;</span>		}
<span id="L205" class="ln">   205&nbsp;&nbsp;</span>		return ea*n1 + esize <span class="comment">// may still overflow to &lt; 0 which is ok</span>
<span id="L206" class="ln">   206&nbsp;&nbsp;</span>	case *Slice:
<span id="L207" class="ln">   207&nbsp;&nbsp;</span>		return s.WordSize * 3
<span id="L208" class="ln">   208&nbsp;&nbsp;</span>	case *Struct:
<span id="L209" class="ln">   209&nbsp;&nbsp;</span>		n := t.NumFields()
<span id="L210" class="ln">   210&nbsp;&nbsp;</span>		if n == 0 {
<span id="L211" class="ln">   211&nbsp;&nbsp;</span>			return 0
<span id="L212" class="ln">   212&nbsp;&nbsp;</span>		}
<span id="L213" class="ln">   213&nbsp;&nbsp;</span>		offsets := s.Offsetsof(t.fields)
<span id="L214" class="ln">   214&nbsp;&nbsp;</span>		offs := offsets[n-1]
<span id="L215" class="ln">   215&nbsp;&nbsp;</span>		size := s.Sizeof(t.fields[n-1].typ)
<span id="L216" class="ln">   216&nbsp;&nbsp;</span>		if offs &lt; 0 || size &lt; 0 {
<span id="L217" class="ln">   217&nbsp;&nbsp;</span>			return -1 <span class="comment">// type too large</span>
<span id="L218" class="ln">   218&nbsp;&nbsp;</span>		}
<span id="L219" class="ln">   219&nbsp;&nbsp;</span>		return offs + size <span class="comment">// may overflow to &lt; 0 which is ok</span>
<span id="L220" class="ln">   220&nbsp;&nbsp;</span>	case *Interface:
<span id="L221" class="ln">   221&nbsp;&nbsp;</span>		<span class="comment">// Type parameters lead to variable sizes/alignments;</span>
<span id="L222" class="ln">   222&nbsp;&nbsp;</span>		<span class="comment">// StdSizes.Sizeof won&#39;t be called for them.</span>
<span id="L223" class="ln">   223&nbsp;&nbsp;</span>		assert(!isTypeParam(T))
<span id="L224" class="ln">   224&nbsp;&nbsp;</span>		return s.WordSize * 2
<span id="L225" class="ln">   225&nbsp;&nbsp;</span>	case *TypeParam, *Union:
<span id="L226" class="ln">   226&nbsp;&nbsp;</span>		unreachable()
<span id="L227" class="ln">   227&nbsp;&nbsp;</span>	}
<span id="L228" class="ln">   228&nbsp;&nbsp;</span>	return s.WordSize <span class="comment">// catch-all</span>
<span id="L229" class="ln">   229&nbsp;&nbsp;</span>}
<span id="L230" class="ln">   230&nbsp;&nbsp;</span>
<span id="L231" class="ln">   231&nbsp;&nbsp;</span><span class="comment">// common architecture word sizes and alignments</span>
<span id="L232" class="ln">   232&nbsp;&nbsp;</span>var gcArchSizes = map[string]*gcSizes{
<span id="L233" class="ln">   233&nbsp;&nbsp;</span>	&#34;386&#34;:      {4, 4},
<span id="L234" class="ln">   234&nbsp;&nbsp;</span>	&#34;amd64&#34;:    {8, 8},
<span id="L235" class="ln">   235&nbsp;&nbsp;</span>	&#34;amd64p32&#34;: {4, 8},
<span id="L236" class="ln">   236&nbsp;&nbsp;</span>	&#34;arm&#34;:      {4, 4},
<span id="L237" class="ln">   237&nbsp;&nbsp;</span>	&#34;arm64&#34;:    {8, 8},
<span id="L238" class="ln">   238&nbsp;&nbsp;</span>	&#34;loong64&#34;:  {8, 8},
<span id="L239" class="ln">   239&nbsp;&nbsp;</span>	&#34;mips&#34;:     {4, 4},
<span id="L240" class="ln">   240&nbsp;&nbsp;</span>	&#34;mipsle&#34;:   {4, 4},
<span id="L241" class="ln">   241&nbsp;&nbsp;</span>	&#34;mips64&#34;:   {8, 8},
<span id="L242" class="ln">   242&nbsp;&nbsp;</span>	&#34;mips64le&#34;: {8, 8},
<span id="L243" class="ln">   243&nbsp;&nbsp;</span>	&#34;ppc64&#34;:    {8, 8},
<span id="L244" class="ln">   244&nbsp;&nbsp;</span>	&#34;ppc64le&#34;:  {8, 8},
<span id="L245" class="ln">   245&nbsp;&nbsp;</span>	&#34;riscv64&#34;:  {8, 8},
<span id="L246" class="ln">   246&nbsp;&nbsp;</span>	&#34;s390x&#34;:    {8, 8},
<span id="L247" class="ln">   247&nbsp;&nbsp;</span>	&#34;sparc64&#34;:  {8, 8},
<span id="L248" class="ln">   248&nbsp;&nbsp;</span>	&#34;wasm&#34;:     {8, 8},
<span id="L249" class="ln">   249&nbsp;&nbsp;</span>	<span class="comment">// When adding more architectures here,</span>
<span id="L250" class="ln">   250&nbsp;&nbsp;</span>	<span class="comment">// update the doc string of SizesFor below.</span>
<span id="L251" class="ln">   251&nbsp;&nbsp;</span>}
<span id="L252" class="ln">   252&nbsp;&nbsp;</span>
<span id="L253" class="ln">   253&nbsp;&nbsp;</span><span class="comment">// SizesFor returns the Sizes used by a compiler for an architecture.</span>
<span id="L254" class="ln">   254&nbsp;&nbsp;</span><span class="comment">// The result is nil if a compiler/architecture pair is not known.</span>
<span id="L255" class="ln">   255&nbsp;&nbsp;</span><span class="comment">//</span>
<span id="L256" class="ln">   256&nbsp;&nbsp;</span><span class="comment">// Supported architectures for compiler &#34;gc&#34;:</span>
<span id="L257" class="ln">   257&nbsp;&nbsp;</span><span class="comment">// &#34;386&#34;, &#34;amd64&#34;, &#34;amd64p32&#34;, &#34;arm&#34;, &#34;arm64&#34;, &#34;loong64&#34;, &#34;mips&#34;, &#34;mipsle&#34;,</span>
<span id="L258" class="ln">   258&nbsp;&nbsp;</span><span class="comment">// &#34;mips64&#34;, &#34;mips64le&#34;, &#34;ppc64&#34;, &#34;ppc64le&#34;, &#34;riscv64&#34;, &#34;s390x&#34;, &#34;sparc64&#34;, &#34;wasm&#34;.</span>
<span id="L259" class="ln">   259&nbsp;&nbsp;</span>func SizesFor(compiler, arch string) Sizes {
<span id="L260" class="ln">   260&nbsp;&nbsp;</span>	switch compiler {
<span id="L261" class="ln">   261&nbsp;&nbsp;</span>	case &#34;gc&#34;:
<span id="L262" class="ln">   262&nbsp;&nbsp;</span>		if s := gcSizesFor(compiler, arch); s != nil {
<span id="L263" class="ln">   263&nbsp;&nbsp;</span>			return Sizes(s)
<span id="L264" class="ln">   264&nbsp;&nbsp;</span>		}
<span id="L265" class="ln">   265&nbsp;&nbsp;</span>	case &#34;gccgo&#34;:
<span id="L266" class="ln">   266&nbsp;&nbsp;</span>		if s, ok := gccgoArchSizes[arch]; ok {
<span id="L267" class="ln">   267&nbsp;&nbsp;</span>			return Sizes(s)
<span id="L268" class="ln">   268&nbsp;&nbsp;</span>		}
<span id="L269" class="ln">   269&nbsp;&nbsp;</span>	}
<span id="L270" class="ln">   270&nbsp;&nbsp;</span>	return nil
<span id="L271" class="ln">   271&nbsp;&nbsp;</span>}
<span id="L272" class="ln">   272&nbsp;&nbsp;</span>
<span id="L273" class="ln">   273&nbsp;&nbsp;</span><span class="comment">// stdSizes is used if Config.Sizes == nil.</span>
<span id="L274" class="ln">   274&nbsp;&nbsp;</span>var stdSizes = SizesFor(&#34;gc&#34;, &#34;amd64&#34;)
<span id="L275" class="ln">   275&nbsp;&nbsp;</span>
<span id="L276" class="ln">   276&nbsp;&nbsp;</span>func (conf *Config) alignof(T Type) int64 {
<span id="L277" class="ln">   277&nbsp;&nbsp;</span>	f := stdSizes.Alignof
<span id="L278" class="ln">   278&nbsp;&nbsp;</span>	if conf.Sizes != nil {
<span id="L279" class="ln">   279&nbsp;&nbsp;</span>		f = conf.Sizes.Alignof
<span id="L280" class="ln">   280&nbsp;&nbsp;</span>	}
<span id="L281" class="ln">   281&nbsp;&nbsp;</span>	if a := f(T); a &gt;= 1 {
<span id="L282" class="ln">   282&nbsp;&nbsp;</span>		return a
<span id="L283" class="ln">   283&nbsp;&nbsp;</span>	}
<span id="L284" class="ln">   284&nbsp;&nbsp;</span>	panic(&#34;implementation of alignof returned an alignment &lt; 1&#34;)
<span id="L285" class="ln">   285&nbsp;&nbsp;</span>}
<span id="L286" class="ln">   286&nbsp;&nbsp;</span>
<span id="L287" class="ln">   287&nbsp;&nbsp;</span>func (conf *Config) offsetsof(T *Struct) []int64 {
<span id="L288" class="ln">   288&nbsp;&nbsp;</span>	var offsets []int64
<span id="L289" class="ln">   289&nbsp;&nbsp;</span>	if T.NumFields() &gt; 0 {
<span id="L290" class="ln">   290&nbsp;&nbsp;</span>		<span class="comment">// compute offsets on demand</span>
<span id="L291" class="ln">   291&nbsp;&nbsp;</span>		f := stdSizes.Offsetsof
<span id="L292" class="ln">   292&nbsp;&nbsp;</span>		if conf.Sizes != nil {
<span id="L293" class="ln">   293&nbsp;&nbsp;</span>			f = conf.Sizes.Offsetsof
<span id="L294" class="ln">   294&nbsp;&nbsp;</span>		}
<span id="L295" class="ln">   295&nbsp;&nbsp;</span>		offsets = f(T.fields)
<span id="L296" class="ln">   296&nbsp;&nbsp;</span>		<span class="comment">// sanity checks</span>
<span id="L297" class="ln">   297&nbsp;&nbsp;</span>		if len(offsets) != T.NumFields() {
<span id="L298" class="ln">   298&nbsp;&nbsp;</span>			panic(&#34;implementation of offsetsof returned the wrong number of offsets&#34;)
<span id="L299" class="ln">   299&nbsp;&nbsp;</span>		}
<span id="L300" class="ln">   300&nbsp;&nbsp;</span>	}
<span id="L301" class="ln">   301&nbsp;&nbsp;</span>	return offsets
<span id="L302" class="ln">   302&nbsp;&nbsp;</span>}
<span id="L303" class="ln">   303&nbsp;&nbsp;</span>
<span id="L304" class="ln">   304&nbsp;&nbsp;</span><span class="comment">// offsetof returns the offset of the field specified via</span>
<span id="L305" class="ln">   305&nbsp;&nbsp;</span><span class="comment">// the index sequence relative to T. All embedded fields</span>
<span id="L306" class="ln">   306&nbsp;&nbsp;</span><span class="comment">// must be structs (rather than pointers to structs).</span>
<span id="L307" class="ln">   307&nbsp;&nbsp;</span><span class="comment">// If the offset is too large (because T is too large),</span>
<span id="L308" class="ln">   308&nbsp;&nbsp;</span><span class="comment">// the result is negative.</span>
<span id="L309" class="ln">   309&nbsp;&nbsp;</span>func (conf *Config) offsetof(T Type, index []int) int64 {
<span id="L310" class="ln">   310&nbsp;&nbsp;</span>	var offs int64
<span id="L311" class="ln">   311&nbsp;&nbsp;</span>	for _, i := range index {
<span id="L312" class="ln">   312&nbsp;&nbsp;</span>		s := under(T).(*Struct)
<span id="L313" class="ln">   313&nbsp;&nbsp;</span>		d := conf.offsetsof(s)[i]
<span id="L314" class="ln">   314&nbsp;&nbsp;</span>		if d &lt; 0 {
<span id="L315" class="ln">   315&nbsp;&nbsp;</span>			return -1
<span id="L316" class="ln">   316&nbsp;&nbsp;</span>		}
<span id="L317" class="ln">   317&nbsp;&nbsp;</span>		offs += d
<span id="L318" class="ln">   318&nbsp;&nbsp;</span>		if offs &lt; 0 {
<span id="L319" class="ln">   319&nbsp;&nbsp;</span>			return -1
<span id="L320" class="ln">   320&nbsp;&nbsp;</span>		}
<span id="L321" class="ln">   321&nbsp;&nbsp;</span>		T = s.fields[i].typ
<span id="L322" class="ln">   322&nbsp;&nbsp;</span>	}
<span id="L323" class="ln">   323&nbsp;&nbsp;</span>	return offs
<span id="L324" class="ln">   324&nbsp;&nbsp;</span>}
<span id="L325" class="ln">   325&nbsp;&nbsp;</span>
<span id="L326" class="ln">   326&nbsp;&nbsp;</span><span class="comment">// sizeof returns the size of T.</span>
<span id="L327" class="ln">   327&nbsp;&nbsp;</span><span class="comment">// If T is too large, the result is negative.</span>
<span id="L328" class="ln">   328&nbsp;&nbsp;</span>func (conf *Config) sizeof(T Type) int64 {
<span id="L329" class="ln">   329&nbsp;&nbsp;</span>	f := stdSizes.Sizeof
<span id="L330" class="ln">   330&nbsp;&nbsp;</span>	if conf.Sizes != nil {
<span id="L331" class="ln">   331&nbsp;&nbsp;</span>		f = conf.Sizes.Sizeof
<span id="L332" class="ln">   332&nbsp;&nbsp;</span>	}
<span id="L333" class="ln">   333&nbsp;&nbsp;</span>	return f(T)
<span id="L334" class="ln">   334&nbsp;&nbsp;</span>}
<span id="L335" class="ln">   335&nbsp;&nbsp;</span>
<span id="L336" class="ln">   336&nbsp;&nbsp;</span><span class="comment">// align returns the smallest y &gt;= x such that y % a == 0.</span>
<span id="L337" class="ln">   337&nbsp;&nbsp;</span><span class="comment">// a must be within 1 and 8 and it must be a power of 2.</span>
<span id="L338" class="ln">   338&nbsp;&nbsp;</span><span class="comment">// The result may be negative due to overflow.</span>
<span id="L339" class="ln">   339&nbsp;&nbsp;</span>func align(x, a int64) int64 {
<span id="L340" class="ln">   340&nbsp;&nbsp;</span>	assert(x &gt;= 0 &amp;&amp; 1 &lt;= a &amp;&amp; a &lt;= 8 &amp;&amp; a&amp;(a-1) == 0)
<span id="L341" class="ln">   341&nbsp;&nbsp;</span>	return (x + a - 1) &amp;^ (a - 1)
<span id="L342" class="ln">   342&nbsp;&nbsp;</span>}
<span id="L343" class="ln">   343&nbsp;&nbsp;</span>
</pre><p><a href="sizes.go?m=text">View as plain text</a></p>

