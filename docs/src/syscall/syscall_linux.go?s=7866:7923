<span id="L1194" class="ln">  1194&nbsp;&nbsp;</span>func Setresgid(rgid, egid, sgid int) (err error) {
<span id="L1195" class="ln">  1195&nbsp;&nbsp;</span>	if cgo_libc_setresgid == nil {
