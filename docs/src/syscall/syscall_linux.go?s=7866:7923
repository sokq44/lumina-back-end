<span id="L968" class="ln">   968&nbsp;&nbsp;</span>}
<span id="L969" class="ln">   969&nbsp;&nbsp;</span>
<span id="L970" class="ln">   970&nbsp;&nbsp;</span>func PtraceGetEventMsg(pid int) (msg uint, err error) {
<span id="L971" class="ln">   971&nbsp;&nbsp;</span>	var data _C_long
<span id="L972" class="ln">   972&nbsp;&nbsp;</span>	err = ptracePtr(PTRACE_GETEVENTMSG, pid, 0, unsafe.Pointer(&amp;data))
<span id="L973" class="ln">   973&nbsp;&nbsp;</span>	msg = uint(data)
<span id="L974" class="ln">   974&nbsp;&nbsp;</span>	return
<span id="L975" class="ln">   975&nbsp;&nbsp;</span>}
