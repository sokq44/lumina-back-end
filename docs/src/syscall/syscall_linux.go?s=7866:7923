<span id="L1211" class="ln">  1211&nbsp;&nbsp;</span>		}
<span id="L1212" class="ln">  1212&nbsp;&nbsp;</span>	} else if ret := cgocaller(cgo_libc_setresuid, uintptr(ruid), uintptr(euid), uintptr(suid)); ret != 0 {
<span id="L1213" class="ln">  1213&nbsp;&nbsp;</span>		err = errnoErr(Errno(ret))
<span id="L1214" class="ln">  1214&nbsp;&nbsp;</span>	}
<span id="L1215" class="ln">  1215&nbsp;&nbsp;</span>	return
