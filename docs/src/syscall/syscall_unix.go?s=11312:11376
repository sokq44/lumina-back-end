<span id="L426" class="ln">   426&nbsp;&nbsp;</span>func sendtoInet4(fd int, p []byte, flags int, to *SockaddrInet4) (err error) {
<span id="L427" class="ln">   427&nbsp;&nbsp;</span>	ptr, n, err := to.sockaddr()
<span id="L428" class="ln">   428&nbsp;&nbsp;</span>	if err != nil {
