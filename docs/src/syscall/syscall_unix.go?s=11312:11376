<span id="L484" class="ln">   484&nbsp;&nbsp;</span>
<span id="L485" class="ln">   485&nbsp;&nbsp;</span>func SetsockoptString(fd, level, opt int, s string) (err error) {
<span id="L486" class="ln">   486&nbsp;&nbsp;</span>	var p unsafe.Pointer
<span id="L487" class="ln">   487&nbsp;&nbsp;</span>	if len(s) &gt; 0 {
<span id="L488" class="ln">   488&nbsp;&nbsp;</span>		p = unsafe.Pointer(&amp;[]byte(s)[0])
<span id="L489" class="ln">   489&nbsp;&nbsp;</span>	}
<span id="L490" class="ln">   490&nbsp;&nbsp;</span>	return setsockopt(fd, level, opt, p, uintptr(len(s)))
<span id="L491" class="ln">   491&nbsp;&nbsp;</span>}
<span id="L492" class="ln">   492&nbsp;&nbsp;</span>
<span id="L493" class="ln">   493&nbsp;&nbsp;</span>func SetsockoptTimeval(fd, level, opt int, tv *Timeval) (err error) {
<span id="L494" class="ln">   494&nbsp;&nbsp;</span>	return setsockopt(fd, level, opt, unsafe.Pointer(tv), unsafe.Sizeof(*tv))
<span id="L495" class="ln">   495&nbsp;&nbsp;</span>}
<span id="L496" class="ln">   496&nbsp;&nbsp;</span>
<span id="L497" class="ln">   497&nbsp;&nbsp;</span>func Socket(domain, typ, proto int) (fd int, err error) {
<span id="L498" class="ln">   498&nbsp;&nbsp;</span>	if domain == AF_INET6 &amp;&amp; SocketDisableIPv6 {
<span id="L499" class="ln">   499&nbsp;&nbsp;</span>		return -1, EAFNOSUPPORT
<span id="L500" class="ln">   500&nbsp;&nbsp;</span>	}
<span id="L501" class="ln">   501&nbsp;&nbsp;</span>	fd, err = socket(domain, typ, proto)
<span id="L502" class="ln">   502&nbsp;&nbsp;</span>	return
<span id="L503" class="ln">   503&nbsp;&nbsp;</span>}
<span id="L504" class="ln">   504&nbsp;&nbsp;</span>
<span id="L505" class="ln">   505&nbsp;&nbsp;</span>func Socketpair(domain, typ, proto int) (fd [2]int, err error) {
<span id="L506" class="ln">   506&nbsp;&nbsp;</span>	var fdx [2]int32
<span id="L507" class="ln">   507&nbsp;&nbsp;</span>	err = socketpair(domain, typ, proto, &amp;fdx)
<span id="L508" class="ln">   508&nbsp;&nbsp;</span>	if err == nil {
<span id="L509" class="ln">   509&nbsp;&nbsp;</span>		fd[0] = int(fdx[0])
<span id="L510" class="ln">   510&nbsp;&nbsp;</span>		fd[1] = int(fdx[1])
<span id="L511" class="ln">   511&nbsp;&nbsp;</span>	}
<span id="L512" class="ln">   512&nbsp;&nbsp;</span>	return
<span id="L513" class="ln">   513&nbsp;&nbsp;</span>}
<span id="L514" class="ln">   514&nbsp;&nbsp;</span>
<span id="L515" class="ln">   515&nbsp;&nbsp;</span>func Sendfile(outfd int, infd int, offset *int64, count int) (written int, err error) {
<span id="L516" class="ln">   516&nbsp;&nbsp;</span>	if race.Enabled {
<span id="L517" class="ln">   517&nbsp;&nbsp;</span>		race.ReleaseMerge(unsafe.Pointer(&amp;ioSync))
<span id="L518" class="ln">   518&nbsp;&nbsp;</span>	}
<span id="L519" class="ln">   519&nbsp;&nbsp;</span>	return sendfile(outfd, infd, offset, count)
<span id="L520" class="ln">   520&nbsp;&nbsp;</span>}
<span id="L521" class="ln">   521&nbsp;&nbsp;</span>
<span id="L522" class="ln">   522&nbsp;&nbsp;</span>var ioSync int64
<span id="L523" class="ln">   523&nbsp;&nbsp;</span>
</pre><p><a href="syscall_unix.go?m=text">View as plain text</a></p>

